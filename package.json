{
	"name": "sofie-rundown-editor",
	"version": "1.3.1",
	"private": true,
	"description": "A standalone tool for creating and editing Sofie rundowns.",
	"author": {
		"name": "SuperFlyTV AB",
		"email": "info@superfly.tv",
		"url": "https://superfly.tv"
	},
	"scripts": {
		"prepare": "husky",
		"serve": "vue-cli-service serve",
		"build": "vue-cli-service build",
		"lint": "run lint:raw .",
		"electron:build": "vue-cli-service electron:build",
		"electron:serve": "vue-cli-service electron:serve",
		"license-validate": "license-checker -r --filter MIT --filter 0BSD --filter BSD --filter ISC --filter Apache --filter Unlicense --plain --border ascii",
		"lint:raw": "eslint",
		"validate:dependencies": "yarn npm audit --environment production && yarn license-validate",
		"validate:dev-dependencies": "yarn npm audit --environment development"
	},
	"main": "index.js",
	"dependencies": {
		"@fortawesome/fontawesome-svg-core": "^6.7.2",
		"@fortawesome/free-solid-svg-icons": "^6.7.2",
		"@fortawesome/vue-fontawesome": "^2.0.10",
		"@popperjs/core": "^2.11.8",
		"@sofie-automation/server-core-integration": "1.53.0-nightly-release53-20250423-081454-dc957dd.0",
		"@types/sqlite3": "^5.1.0",
		"@types/uuid": "^8.3.4",
		"bootstrap": "^4.6.2",
		"bootstrap-vue": "^2.23.1",
		"core-js": "^3.42.0",
		"sqlite3": "^5.1.7",
		"uuid": "^8.3.2",
		"vue": "^2.7.16",
		"vue-router": "^3.6.5",
		"vuedraggable": "^2.24.3",
		"vuex": "^3.6.2"
	},
	"devDependencies": {
		"@electron/remote": "^2.1.2",
		"@types/electron-devtools-installer": "^2.2.5",
		"@types/node": "^14.18.63",
		"@typescript-eslint/eslint-plugin": "^5.62.0",
		"@typescript-eslint/parser": "^5.62.0",
		"@volar-plugins/vetur": "^0.1.0",
		"@vue/cli-plugin-babel": "~5.0.8",
		"@vue/cli-plugin-typescript": "~5.0.8",
		"@vue/cli-plugin-vuex": "~5.0.8",
		"@vue/cli-service": "~5.0.8",
		"@vue/eslint-config-prettier": "^6.0.0",
		"@vue/eslint-config-typescript": "^9.1.0",
		"electron": "^19.1.9",
		"electron-devtools-installer": "^3.2.1",
		"eslint": "^7.32.0",
		"eslint-config-prettier": "^8.10.0",
		"eslint-plugin-prettier": "^4.2.1",
		"eslint-plugin-vue": "^8.7.1",
		"husky": "^9.1.7",
		"license-checker": "^25.0.1",
		"lint-staged": "^16.0.0",
		"prettier": "^2.8.8",
		"typescript": "~4.6.4",
		"vue-cli-plugin-electron-builder": "^3.0.0-alpha.4",
		"vue-template-compiler": "^2.7.16"
	},
	"resolutions": {
		"vue-cli-plugin-electron-builder/electron-builder": "^23.0.3"
	},
	"license": "MIT",
	"lint-staged": {
		"*.{js,css,json,md,scss}": [
			"prettier --write"
		],
		"*.{ts,tsx,.vue}": [
			"yarn lint:raw --fix"
		]
	},
	"packageManager": "yarn@4.9.1"
}
